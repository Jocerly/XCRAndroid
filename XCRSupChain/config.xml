<?xml version='1.0' encoding='utf-8'?>
<widget id="com.yatang.xc.supchain" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>XCRSupChain</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <preference name="DisallowOverscroll" value="true" />
    <engine name="android" spec="~6.2.3" />
    <engine name="ios" spec="^4.4.0" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <plugin name="com-yatang-plugin-ytlogicservice" spec="D:\yatang\gitrepo\other\com-yatang-plugin-ytlogicservice" />
    <plugin name="com-yatang-plugin-ytnavigation" spec="D:\yatang\gitrepo\other\com-yatang-plugin-ytnavigation" />
    <plugin name="com-yatang-plugin-ytnetwork" spec="D:\yatang\gitrepo\other\com-yatang-plugin-ytnetwork" />
    <plugin name="com-yatang-plugin-ytnotification" spec="D:\yatang\gitrepo\other\com-yatang-plugin-ytnotification" />
    <plugin name="com-yatang-plugin-ytpay" spec="D:\yatang\gitrepo\other\com-yatang-plugin-ytpay" />
    <plugin name="x5webview-cordova-plugin" spec="https://github.com/runner525/x5webview-cordova-plugin" />
</widget>
