<?xml version='1.0' encoding='utf-8'?>
<widget id="com.yatang.xc.supchain" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <name>XCRSupChain</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="market:*" />
    <allow-navigation href="http://*/*" />
    <allow-navigation href="https://*/*" />
    <preference name="loglevel" value="DEBUG" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="loadUrlTimeoutValue" value="700000" />
    <feature name="YTLogicService">
        <param name="android-package" value="com.yatang.plugin.ytlogicservice.YTLogicService" />
    </feature>
    <feature name="YTNavigation">
        <param name="android-package" value="com.yatang.plugin.navigation.APager" />
    </feature>
    <feature name="YTNetwork">
        <param name="android-package" value="com.yatang.plugin.ytnetwork.YTNetwork" />
    </feature>
    <feature name="YTNotification">
        <param name="android-package" value="com.yatang.plugin.ytnotification.YTNotification" />
    </feature>
    <feature name="YTPay">
        <param name="android-package" value="com.yatang.plugin.ytpay.YTPay" />
    </feature>
    <preference name="webView" value="org.apache.cordova.x5engine.X5WebViewEngine" />
    <preference name="appUserAgent" value="x5webview" />
    <preference name="android-minSdkVersion" value="15" />
    <allow-navigation href="*" />
</widget>
